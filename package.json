{
  "name": "coc-calc",
  "version": "1.2.1",
  "description": "calculate extension for coc.nvim",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim",
    "calculator",
    "calc"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsc",
    "prepare": "yarn run clean && yarn run build",
    "test": "jest"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "coc": "^0.0.71"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Calc",
      "properties": {
        "calc.debug": {
          "type": "boolean",
          "default": false
        },
        "calc.priority": {
          "type": "integer",
          "default": 99,
          "description": "Calc priority"
        },
        "calc.highlight": {
          "type": "boolean",
          "description": "Enable calc highlight",
          "default": true
        }
      }
    }
  },
  "license": "MIT",
  "author": "weirongxu.raidou@gmail.com",
  "publisher": "weirongxu",
  "homepage": "https://github.com/weirongxu/coc-calc",
  "repository": {
    "type": "git",
    "url": "https://github.com/weirongxu/coc-calc.git"
  },
  "devDependencies": {
    "@chemzqm/tslint-config": "^1.0.18",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/parsimmon": "^1.10.0",
    "coc.nvim": "^0.0.72",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "decimal.js": "^10.2.0",
    "parsimmon": "^1.12.0",
    "vscode-languageserver-protocol": "^3.14.1"
  }
}
